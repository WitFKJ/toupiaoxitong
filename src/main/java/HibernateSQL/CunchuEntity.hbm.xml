<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="HibernateSQL.CunchuEntity" table="cunchu" schema="toupiaoxiong">
        <property name="cunchuUserNum">
            <column name="cunchu_user_num" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="cunchuWenjuanNum">
            <column name="cunchu_wenjuan_num" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="cunchuQuestNum">
            <column name="cunchu_quest_num" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="cunchuXuanxiangClass">
            <column name="cunchu_xuanxiang_class" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="cunchuXuanxiangPick">
            <column name="cunchu_xuanxiang_pick" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>